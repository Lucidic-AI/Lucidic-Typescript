{
  "name": "lucidicai",
  "version": "2.0.5",
  "description": "Lucidic AI TypeScript SDK for AI Agent Observability",
  "author": {
    "name": "Anvit Sinha",
    "email": "anvit@lucidic.ai"
  },
  "contributors": [
    {
      "name": "Lucidic AI",
      "email": "team@lucidic.ai"
    }
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run clean && npm run build && npm run typecheck && npx publint"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.11",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-async-hooks": "^1.30.1",
    "@opentelemetry/instrumentation": "^0.57.2",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@traceloop/instrumentation-anthropic": "^0.12.0",
    "@traceloop/instrumentation-bedrock": "^0.12.0",
    "@traceloop/instrumentation-cohere": "^0.12.0",
    "@traceloop/instrumentation-langchain": "^0.12.0",
    "@traceloop/instrumentation-vertexai": "^0.12.0",
    "@traceloop/instrumentation-openai": "^0.12.0",
    "ai": "^5.0.11",
    "jimp": "^0.22.12",
    "lucidicai": "^2.0.3"
  },
  "devDependencies": {
    "@google-cloud/vertexai": "^1.0.0",
    "@anthropic-ai/sdk": "^0.39.0",
    "dotenv": "^16.4.5",
    "openai": "^4.48.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
